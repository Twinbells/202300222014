/*
*
*Store by row or column
*
*
*/

#include <stdio.h>

int main() {
    int A[2][3] = {{1, 2, 3}, {4, 5, 6}};
    for (int i = 0; i < 2; ++i) {
        for (int j = 0; j < 3; ++j) {
            printf("A[%d][%d] = %d, Address: %p\n", i, j, A[i][j], (void *)&A[i][j]);
        }
    }

    return 0;
}
